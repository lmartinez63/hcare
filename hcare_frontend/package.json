{
  "name": "hcare_frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "pre-build": "rimraf ./node_modules/.cache",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached --config webpack.config.js",
    "lint": "vue-cli-service lint",
    "start": "webpack-dev-server --host 0.0.0.0 --port 8081 --open"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "html-webpack-plugin": "^3.2.0",
    "luxon": "^1.7.1",
    "moment": "^2.22.2",
    "vue": "^2.5.17",
    "vue-datetime": "^1.0.0-beta.8",
    "vue-loader": "^14.2.2",
    "vue-multiselect": "^2.1.4",
    "vue-round-filter": "^1.1.2",
    "vue-router": "^3.0.1",
    "vue-search-select": "^2.8.3",
    "vuejs-datepicker": "^1.5.4",
    "vuelidate": "^0.7.4",
    "vuex": "^3.0.1",
    "webpack-cli": "^3.2.1",
    "weekstart": "^1.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "babel-eslint": "^10.0.1",
    "css-loader": "^2.1.0",
    "eslint": "^5.9.0",
    "eslint-config-google": "^0.11.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
